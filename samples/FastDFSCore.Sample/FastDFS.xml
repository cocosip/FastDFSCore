<FDFSClient>
  <![CDATA[Trackers信息]]>
  <Trackers>
    <Tracker>
      <IPAddress>192.168.0.6</IPAddress>
      <Port>22122</Port>
    </Tracker>
  </Trackers>
  <![CDATA[编码]]>
  <Charset>utf-8</Charset>
  <![CDATA[连接超时时间,5s]]>
  <ConnectionTimeout>5</ConnectionTimeout>
  <![CDATA[连接的有效时间,3600s]]>
  <ConnectionLifeTime>3600</ConnectionLifeTime>
  <![CDATA[查询超时的连接的时间间隔,10s]]>
  <ScanTimeoutConnectionInterval>10</ScanTimeoutConnectionInterval>
  <![CDATA[Tracker最大的连接数]]>
  <TrackerMaxConnection>50</TrackerMaxConnection>
  <![CDATA[Storage最大连接数]]>
  <StorageMaxConnection>100</StorageMaxConnection>
  <![CDATA[日志名称]]>
  <LoggerName>FDFSLogger</LoggerName>
  <![CDATA[Socket相关数据配置]]>
  <TcpSetting>
    <![CDATA[退出后在指定时间内关闭,ms为单位]]>
    <QuietPeriodMilliSeconds>100</QuietPeriodMilliSeconds>
    <![CDATA[超时关闭时间,以s为单位]]>
    <CloseTimeoutSeconds>1</CloseTimeoutSeconds>
    <![CDATA[高水位,16Mb]]>
    <WriteBufferHighWaterMark>16777216</WriteBufferHighWaterMark>
    <![CDATA[低水位,8Mb]]>
    <WriteBufferLowWaterMark>8388608</WriteBufferLowWaterMark>
    <![CDATA[接收的Socket缓存大小,1Mb]]>
    <SoRcvbuf>1048576</SoRcvbuf>
    <![CDATA[发送的Socket缓存大小,1Mb]]>
    <SoSndbuf>1048576</SoSndbuf>
    <![CDATA[Tcp发送是否无延迟]]>
    <TcpNodelay>true</TcpNodelay>
    <![CDATA[是否重用端口]]>
    <SoReuseaddr>false</SoReuseaddr>
    <![CDATA[是否开启客户端重连机制]]>
    <EnableReConnect>true</EnableReConnect>
    <![CDATA[重连延迟秒数]]>
    <ReConnectDelaySeconds>2</ReConnectDelaySeconds>
    <![CDATA[重连时间间隔]]>
    <ReConnectIntervalMilliSeconds>1000</ReConnectIntervalMilliSeconds>
    <![CDATA[最大重连次数]]>
    <ReConnectMaxCount>10</ReConnectMaxCount>
  </TcpSetting>
</FDFSClient>