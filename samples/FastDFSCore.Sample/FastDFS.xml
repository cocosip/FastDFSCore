<FDFSClient>
  <![CDATA[Trackers信息]]>
  <Trackers>
    <Tracker>
      <IPAddress>192.168.0.7</IPAddress>
      <Port>22122</Port>
    </Tracker>
  </Trackers>
  <![CDATA[编码]]>
  <Charset>utf-8</Charset>
  <![CDATA[连接超时时间,5s]]>
  <ConnectionTimeout>5</ConnectionTimeout>
  <![CDATA[连接的有效时间,3600s]]>
  <ConnectionLifeTime>3600</ConnectionLifeTime>
  <![CDATA[查询超时的连接的时间间隔,10s]]>
  <ScanTimeoutConnectionInterval>10</ScanTimeoutConnectionInterval>
  <![CDATA[Tracker最大的连接数]]>
  <TrackerMaxConnection>1</TrackerMaxConnection>
  <![CDATA[Storage最大连接数]]>
  <StorageMaxConnection>3</StorageMaxConnection>
  <![CDATA[Socket相关数据配置]]>
  <TcpSetting>
    <![CDATA[Tcp发送是否无延迟]]>
    <TcpNodelay>true</TcpNodelay>
    <![CDATA[是否重用端口]]>
    <SoReuseaddr>false</SoReuseaddr>
    <![CDATA[是否开启客户端重连机制]]>
    <EnableReConnect>true</EnableReConnect>
    <![CDATA[重连延迟秒数]]>
    <ReConnectDelaySeconds>2</ReConnectDelaySeconds>
    <![CDATA[重连时间间隔]]>
    <ReConnectIntervalMilliSeconds>1000</ReConnectIntervalMilliSeconds>
