<FDFSClient>
  <![CDATA[Trackers信息]]>
  <Trackers>
    <Tracker>
      <IPAddress>127.1.1.1</IPAddress>
      <Port>22122</Port>
    </Tracker>
  </Trackers>
  <![CDATA[编码]]>
  <Charset>utf-8</Charset>
